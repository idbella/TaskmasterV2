[
	{
		"name" : "nginx",
		"cmd": "/usr/local/bin/nginx -c /etc/nginx/test.conf",
		"numprocs": 1,
		"umask": 22,
		"workingdir": "/tmp",
		"autostart": true,
		"autorestart": "unexpected",
		"exitcodes":
		[
			0,
			2,
			255
		],
		"startretries": 3,
		"starttime": 5,
		"stopsignal": "TERM",
		"stoptime": 10,
		"stdout": "/tmp/nginx.stdout",
		"stderr": "/tmp/nginx.stderr",
		"env":[
			{
				"key":"STARTED_BY",
				"value":"taskmaster"
			},
			{
				"key":"ANSWER",
				"value":42
			}        
		]
	}
]

